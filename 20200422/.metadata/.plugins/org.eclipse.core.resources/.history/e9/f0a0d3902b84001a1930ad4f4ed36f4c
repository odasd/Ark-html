<body>
	<table id="tbl">
		<tr class="row">
			<td>1</td>
			<td><button sname="홍길동" address="인천">출력</button></td>
		</tr>
		<tr class="row">
			<td>2</td>
			<td><button sname="심청이" address="서울">출력</button></td>
		</tr>
	</table>
	
	<div>
		<h1 id="sname"></h1>
		<h1 id="address"></h1>
	</div>
</body>
<script>
	$("#tbl").on("click", ".row button" ,function(){
		var sname=$(this).prop("sname");
		var address=$(this).attr("address");
		alert(sname + address);
	});
</script>
